{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "description": "This is a food delivery service app for a restaurant",
  "main": "index.js",
  "eslintIgnore": [
    "package-lock.json",
    "package.json",
    "README.md",
    "node_modules"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "start": "nodemon --exec babel-node --presets es2015,stage-2 ./server/",
    "test": "set NODE_ENV=test&&nyc mocha --timeout 10000 --compilers js:babel-core/register --require babel-polyfill ./server/test/**/*.test.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "cover": "./node_modules/coveralls/bin/coveralls",
    "lint": "eslint ./node_modules/.bin/eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jideajayi11/Fast-Food-Fast.git"
  },
  "author": "jide ajayi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jideajayi11/Fast-Food-Fast/issues"
  },
  "homepage": "https://github.com/jideajayi11/Fast-Food-Fast#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "istanbul": "^0.4.5",
    "lint": "^1.1.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "prettier": "^1.14.2"
  }
}
